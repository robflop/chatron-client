{
  "name": "chatron",
  "version": "2.0.0",
  "productName": "Chatron",
  "description": "A chatroom client built with electron and vuejs.",
  "main": "./src/client",
  "dependencies": {
    "vue-chat-scroll": "^1.2.1"
  },
  "devDependencies": {
